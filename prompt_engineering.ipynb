        "from langchain_google_genai import ChatGoogleGenerativeAI\n",
        "from langchain.chains import LLMChain\n",
        "from langchain.prompts import PromptTemplate\n",
        "import os\n",
        "\n",
        "# Set your Google API key\n",
        "os.environ[\"GOOGLE_API_KEY\"] =use your own api key,
        "\n",
        "# Define the prompt template\n",
        "template = '''I want you to act as a medical assistant for the people and in an easy way\n",
        "explain the basics of {medical_disease}.'''\n",
        "\n",
        "# Create a PromptTemplate instance\n",
        "prompt = PromptTemplate(\n",
        "    template=template,\n",
        "    input_variables=[\"medical_disease\"]  # Define input variable\n",
        ")\n",
        "\n",
        "# Initialize the ChatGoogleGenerativeAI model\n",
        "llm = ChatGoogleGenerativeAI(model=\"gemini-pro\")\n",
        "\n",
        "# Create the LLMChain\n",
        "chain = LLMChain(\n",
        "    llm=llm,\n",
        "    prompt=prompt\n",
        ")\n",
        "\n",
        "# Run the chain with an input\n",
        "response = chain.run(medical_disease=\"Dengue\")\n",
        "print(response)\n"
